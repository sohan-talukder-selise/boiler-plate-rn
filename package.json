{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-16PM": "react-native run-ios --simulator='iPhone 16 Pro Max'",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --write src --ignore-path .gitignore",
    "start": "react-native start",
    "test": "jest",
    "format:fix": "prettier --write . \"!**/*.{.json}\" --ignore-path .gitignore .",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@realm/react": "^0.11.0",
    "@reduxjs/toolkit": "^2.2.8",
    "@types/react-native-calendar-picker": "^8.0.0",
    "date-fns": "^4.1.0",
    "moment-timezone": "^0.5.46",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-calendar-picker": "^8.0.5",
    "react-native-device-info": "^13.0.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-image-crop-picker": "^0.41.3",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.12.3",
    "react-redux": "^9.1.2",
    "realm": "^12.13.1",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@realm/babel-plugin": "^0.2.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,css,scss,md}": "prettier --write . \"!**/*.{.json}\" --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  }
}
